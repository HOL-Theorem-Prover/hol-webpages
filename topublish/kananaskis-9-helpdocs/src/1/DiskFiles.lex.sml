| 35 => (Tokens.TMC(!pos,!pos))
| 39 => (Tokens.IDS(!pos,!pos))
| 4 => (continue())
| 45 => (Tokens.TYPES(!pos,!pos))
| 51 => (Tokens.TERMS(!pos,!pos))
| 6 => (Tokens.DOLLAR(!pos,!pos))
| 60 => (Tokens.THEOREMS(!pos,!pos))
